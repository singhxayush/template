{
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "hono/jsx",
    "rootDir": "./src",
    "paths": {
      "@/*": ["./src/*"],
      "@/context/*": ["./src/context/*"],
      "@/db/*": ["./src/db/*"],
      "@/utils/*": ["./src/shared/utils/*"]
    },
    "strict": true,
    "outDir": "./dist"
  },
  "include": ["src", "kysely.config.ts"],
  "exclude": ["node_modules"]
}
